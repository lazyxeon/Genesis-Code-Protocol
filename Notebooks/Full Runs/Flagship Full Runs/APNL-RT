BEGIN ARTIFACT:Policies/prompt.rego
 package gcp.prompt
 default allow = false
 deny[msg] { contains(lower(input.prompt), "password"); msg := "Secret-seeking" }
 deny[msg] { contains(lower(input.prompt), "bypass safety"); msg := "Safety bypass attempt" }
 deny[msg] { input.output.contains_claim; not input.output.claim_ids_valid; msg := "Ungrounded claim" }
 allow { not deny[_] }
 END ARTIFACT
